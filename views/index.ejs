    <p>Ref: #2352</p>
    <div id="paypal"></div>

    <div id="paypal-container">
      <div id="message" class="hidden">
        <p>Thank you! Transaction successful</p>
        <p>Transaction ID: <span id="transaction-id"></span></p>
      </div>
    </div>

  </div>
